@model Equipe
@{
    ViewData["Ttle"] = "Edit";
}
<h2>Edição de Equipe</h2>

<form method="post" action="@Url.Action("Atualizar")" enctype="multipart/form-data">

     <input type="hidden" name="idEquipe" value="@ViewBag
.Equipe.idEquipe">

    <label for="">Nome</label>
    <input type="text" name="Nome" value="@ViewBag
    .Equipe.Nome">
    
    <label for="">Imagem</label>
    <input type="file" name ="Imagem" value="@ViewBag.Equipe.imagem">
    

    <button type="submit">Atualizar</button>
</form>